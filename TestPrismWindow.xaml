<Window x:Class="AddinTest.TestPrismWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AddinTest"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=local:TestPrismWindowViewModel}"
        Title="TestPrismWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="TestBtn" Width="150" Height="35" Content="测试" Command="{Binding TestCommand}" HorizontalAlignment="Center" VerticalAlignment="Center"></Button>
    </Grid>
</Window>
